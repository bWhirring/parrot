{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  },
  "rules": {
    "react/no-array-index-key": 0,
    "quotes": 0,
    "global-require": 0,
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx"]
    }],
    "no-unreachable": 0,
    "react/destructuring-assignment": 0,
    "react/prop-types": 0,
    "react/button-has-type": 0
  }
}
