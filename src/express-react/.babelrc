{
  "presets": [
    "react",
    "stage-0",
    "es2015", [
      "env",
      {
        "modules": false,
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    ["import", {
      "libraryName": "antd",
      "style": true
    }]
  ]
}
